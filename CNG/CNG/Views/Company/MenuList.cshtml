@model IEnumerable<CNG.Models.Company>

<ul class="sub-menu">
    @foreach (var item in Model)
    {
        <li>
            <a href="@Url.Action("Dashboard", "Home", new { companyId = item.Id })"><i class="fa fa-building"></i> <span class="title">@item.Name</span> </a>
        </li>
    }
</ul>